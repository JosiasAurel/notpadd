{
  "name": "notpadd-vite",
  "description": "Use notpadd with Vite",
  "version": "0.2.4",
  "type": "module",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts -d dist"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^6.2.0"
  },
  "peerDependencies": {
    "notpadd": "workspace:*",
    "vite": "^5"
  }
}
